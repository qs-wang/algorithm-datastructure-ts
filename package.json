{
  "name": "alg-js",
  "version": "0.0.1",
  "description": "Implements the classic alg in nodejs",
  "engineStrict": true,
  "engines": {
    "node": ">= 8.9.0"
  },
  "devDependencies": {
    "@types/jest": "~21.1.5",
    "@types/node": "~8.0.49",
    "jest": "~21.2.1",
    "rimraf": "~2.6.2",
    "tslint": "~5.8.0",
    "tslint-microsoft-contrib": "~5.0.1",
    "tsutils": "~2.12.2",
    "typescript": "~2.6.1"
